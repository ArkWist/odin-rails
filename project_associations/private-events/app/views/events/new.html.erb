<% provide(:title, "New event") %>
<h1>Events#new</h1>
<p>Find me in app/views/events/new.html.erb</p>

<h1>Create event</h1>

<%= form_for :event do |f| %>
  
  <%= f.label :title %>
  <%= f.text_field :title %>
  
  <%= f.label :date %>
  <%= f.date_field :date %>
  
  <%= f.submit "Create event" %>
  
<% end %>